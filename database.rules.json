{
  "rules": {
    "marks-app": {
      "$uid": {
        "subjects": {
          ".indexOn": ["name"]
        },
        "tests": {
          ".indexOn": ["dueDate"]
        },
        "marks": {
          ".indexOn": ["subjectId"]
        }
      }
    },
    ".read": "auth.uid !== null",
    ".write": "auth.uid !== null"
  }
}